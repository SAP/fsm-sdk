{
  "name": "fsm-sdk",
  "version": "1.0.1",
  "description": "Node.JS sdk to interface with SAP Field Service Management APIs.",
  "readme": "./README.md",
  "main": "release/CoreAPIClient.js",
  "types": "release/CoreAPIClient.d.ts",
  "scripts": {
    "prestart": "npm run build",
    "start": "node release/index.js",
    "prestart:examples": "npm run build",
    "start:examples": "node -r dotenv/config examples/index.js",
    "prebuild": "npm run clean",
    "build": "tsc -p . --pretty",
    "clean": "rm -rf release",
    "test": "nyc mocha --opts mocha.opts",
    "posttest": "cat ./coverage/lcov.info | COVERALLS_REPO_TOKEN=$COVERALLS_REPO_TOKEN COVERALLS_SERVICE_NAME=travis-pro ./node_modules/coveralls/bin/coveralls.js",
    "beforepublish": "echo //registry.npmjs.org/:_authToken=${NPM_TOKEN} > .npmrc && npm run test && npm run clean && npm run build && npm run start:examples"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "statements": 44,
    "branches": 49,
    "functions": 38,
    "lines": 43,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "**/model/*.*",
      "**/test/*.*",
      "**/*.d.ts",
      "**/*.js",
      "**/*.spec.ts",
      "src/index.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "all": true,
    "reporter": [
      "text-summary",
      "text",
      "html",
      "lcovonly"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sap/fsm-sdk.git"
  },
  "author": "SAP SE or an SAP affiliate company - Simon Gausmann",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sap/fsm-sdk/issues"
  },
  "homepage": "https://github.com/sap/fsm-sdk#readme",
  "dependencies": {
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.3",
    "@types/request": "^2.47.1",
    "@types/request-promise-native": "^1.0.15",
    "@types/uuid": "^3.4.4",
    "coveralls": "^3.0.2",
    "dotenv": "^6.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
  }
}